<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bibu.product.dal.mapper.LabelMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bibu.product.dal.entity.Label">
        <id column="id" property="id" />
        <result column="label_type" property="labelType" />
        <result column="label_name" property="labelName" />
        <result column="update_time" property="updateTime" />
        <result column="create_time" property="createTime" />
        <result column="updater" property="updater" />
        <result column="creator" property="creator" />
        <result column="is_delete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, label_type, label_name, update_time, create_time, updater, creator, is_delete
    </sql>

</mapper>
